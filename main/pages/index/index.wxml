<swiper indicator-dots="true" autoplay="true" interval="3000" duration="750" circular="true">
  <block wx:for="{{imgUrls}}" wx:key="*image">
    <swiper-item style='background-color: orange;'>
      <image class="swiper-item" src="{{item}}" mode='aspectFit' />
    </swiper-item>
  </block>
</swiper>

<navigator class='search-navi' url='search/search'>
  <image class="search-img" src="../../image/search.png" />
  <text style='font-size: 28px'>订单查询</text>
</navigator>

<navigator class='search-navi' url='webview/webview' style='background-color: #c7ffec'>
  <view>X X X X</view>
</navigator>

